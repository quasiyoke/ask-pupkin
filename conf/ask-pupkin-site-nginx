server {
	listen 80 default_server;
	listen [::]:80 default_server ipv6only=on;

	server_name localhost;

	location /uploads {
	    root /home/quasiyoke/git/ask-pupkin;
		try_files $uri =404;
		expires 1w;
	}

	location ~* \.(css|js|jpeg|jpg|png|svg|gif|ico)$ {
	    root /home/quasiyoke/git/ask-pupkin/static;
		try_files $uri =404;
		expires 1M;
	}

	location / {
		proxy_pass http://127.0.0.1:8080;    
	}
}
